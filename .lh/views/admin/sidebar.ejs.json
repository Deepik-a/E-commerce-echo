            "date": 1736751034996,
            "name": "Commit-0",
            "content": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n  <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel=\"stylesheet\">\r\n  <link href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css\" rel=\"stylesheet\">\r\n  <style>\r\n    .bed-sidebar {\r\n      position: fixed;\r\n      top: 0;\r\n      left: 0;\r\n      bottom: 0;\r\n      width: 250px;\r\n      z-index: 999;\r\n      transition: all 0.3s;\r\n      background-color: #f8f9fa;\r\n      overflow-y: auto;\r\n      height: 100vh;\r\n      box-shadow: 2px 0 5px rgba(0,0,0,0.1);\r\n    }\r\n\r\n    .bed-nav {\r\n      height: 100%;\r\n      padding: 0;\r\n      display: flex;\r\n      flex-direction: column;\r\n    }\r\n\r\n    .bed-nav__content {\r\n      display: flex;\r\n      flex-direction: column;\r\n      height: 100%;\r\n      padding-top: 1rem;\r\n    }\r\n\r\n    .bed-brand {\r\n      padding: 1rem 1.5rem;\r\n      margin-bottom: 1rem;\r\n    }\r\n\r\n    .bed-user {\r\n      padding: 0 1.5rem;\r\n      margin-bottom: 1.5rem;\r\n    }\r\n\r\n    .bed-user__profile {\r\n      position: relative;\r\n      width: 40px;\r\n      height: 40px;\r\n    }\r\n\r\n    .bed-user__status {\r\n      position: absolute;\r\n      right: 0;\r\n      bottom: 0;\r\n      width: 10px;\r\n      height: 10px;\r\n    }\r\n\r\n    .bed-user__info h6 {\r\n      font-weight: 700;\r\n      color: #2c3e50;\r\n      margin: 0;\r\n    }\r\n\r\n    .bed-nav__section {\r\n      flex-grow: 1;\r\n      display: flex;\r\n      flex-direction: column;\r\n      justify-content: space-between;\r\n    }\r\n\r\n    .bed-menu {\r\n      flex-grow: 1;\r\n    }\r\n\r\n    .bed-menu__item {\r\n      font-weight: 600;\r\n      color: #2c3e50;\r\n      padding: 0.7rem 1.5rem;\r\n      font-size: 1.05rem;\r\n    }\r\n\r\n    .bed-menu__item:hover {\r\n      background-color: #e9ecef;\r\n      color: #0d6efd;\r\n    }\r\n\r\n    .bed-menu__item--active {\r\n      background-color: #0d6efd;\r\n      color: white !important;\r\n    }\r\n\r\n    .bed-submenu {\r\n      padding-left: 3.5rem;\r\n    }\r\n\r\n    .bed-logout {\r\n      padding: 0 1.5rem;\r\n      margin-top: auto;\r\n      border-top: 1px solid #dee2e6;\r\n      padding-top: 1rem;\r\n    }\r\n\r\n    /* New styles for toggle button */\r\n    .sidebar-toggle {\r\n      position: fixed;\r\n      top: 1rem;\r\n      left: 1rem;\r\n      z-index: 1000;\r\n      display: none;\r\n      padding: 0.5rem;\r\n      background-color: #0d6efd;\r\n      color: white;\r\n      border: none;\r\n      border-radius: 4px;\r\n      cursor: pointer;\r\n    }\r\n\r\n    /* Main content wrapper */\r\n    .content-wrapper {\r\n      margin-left: 250px;\r\n      transition: all 0.3s;\r\n      padding: 20px;\r\n    }\r\n\r\n    @media (max-width: 992px) {\r\n      .bed-sidebar {\r\n        margin-left: -250px;\r\n      }\r\n      \r\n      .bed-sidebar.active {\r\n        margin-left: 0;\r\n      }\r\n\r\n      .sidebar-toggle {\r\n        display: block;\r\n      }\r\n\r\n      .content-wrapper {\r\n        margin-left: 0;\r\n      }\r\n\r\n      .content-wrapper.active {\r\n        margin-left: 250px;\r\n      }\r\n    }\r\n  </style>\r\n</head>\r\n<body>\r\n    <button class=\"sidebar-toggle\" id=\"sidebarToggle\">\r\n        <i class=\"fas fa-bars\"></i>\r\n      </button>\r\n  <div class=\"bed-sidebar\" id=\"bedSidebar\">\r\n    <nav class=\"bed-nav navbar bg-light navbar-light\" id=\"bedMainNav\">\r\n      <div class=\"bed-nav__content\">\r\n        <!-- Brand Section -->\r\n        <div class=\"bed-brand\" id=\"bedBrand\">\r\n          <a href=\"index.html\" class=\"navbar-brand\">\r\n            <h3 class=\"text-primary\"><i class=\"fas fa-home me-2\"></i>Dashboard</h3>\r\n          </a>\r\n        </div>\r\n\r\n        <!-- User Section -->\r\n        <div class=\"bed-user\" id=\"bedUserSection\">\r\n          <div class=\"d-flex align-items-center\">\r\n            <div class=\"bed-user__profile\">\r\n              <div class=\"bed-user__avatar d-flex align-items-center justify-content-center\">\r\n                <i class=\"fas fa-user-circle fa-2x text-primary\"></i>\r\n              </div>\r\n              <div class=\"bed-user__status bg-success rounded-circle border border-2 border-white\"></div>\r\n            </div>\r\n            <div class=\"bed-user__info ms-3\">\r\n              <h6 class=\"mb-0\">DEEPIKA PS</h6>\r\n              <span>Admin</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Navigation Section -->\r\n        <div class=\"bed-nav__section\">\r\n          <div class=\"bed-menu\" id=\"bedMainMenu\">\r\n            <a href=\"index.html\" class=\"nav-link bed-menu__item bed-menu__item--active\">\r\n              <i class=\"fas fa-tachometer-alt me-2\"></i>Dashboard\r\n            </a>\r\n            \r\n            <a href=\"/admin/salesReport\" class=\"nav-link bed-menu__item\">\r\n              <i class=\"fas fa-chart-line me-2\"></i>Sales Report\r\n            </a>\r\n\r\n          \r\n                  <a href=\"/admin/addCategory\" class=\"nav-link bed-menu__item\">\r\n                    <i class=\"fas fa-plus-circle me-2\"></i>Add Category\r\n                  </a>\r\n                  <a href=\"/admin/editCategory\" class=\"nav-link bed-menu__item\">\r\n                    <i class=\"fas fa-edit me-2\"></i>Category List\r\n                  </a>\r\n            <a href=\"/admin/products\" class=\"nav-link bed-menu__item\" id=\"bedCustomersLink\">\r\n              <i class=\"fas fa-box me-2\"></i>Products\r\n            </a>\r\n            <a href=\"/admin/users\" class=\"nav-link bed-menu__item\" id=\"bedCustomersLink\">\r\n              <i class=\"fas fa-users me-2\"></i>Customers\r\n            </a>\r\n            <a href=\"/admin/coupons\" class=\"nav-link bed-menu__item\" id=\"bedCouponsLink\">\r\n              <i class=\"fas fa-ticket-alt me-2\"></i>Coupons\r\n            </a>\r\n            <a href=\"/admin/orders\" class=\"nav-link bed-menu__item\" id=\"bedOrdersLink\">\r\n              <i class=\"fas fa-shopping-cart me-2\"></i>Orders\r\n            </a>\r\n            <a href=\"/admin/offer-management\" class=\"nav-link bed-menu__item\" id=\"bedOffersLink\">\r\n              <i class=\"fas fa-gift me-2\"></i>Offers\r\n            </a>\r\n          \r\n            <a href=\"/admin/logout\" class=\"nav-link bed-menu__item\">\r\n              <i class=\"fas fa-sign-out-alt me-2\"></i>Logout\r\n            </a>\r\n          </div>\r\n\r\n          \r\n         \r\n          \r\n        </div>\r\n      </div>\r\n    </nav>\r\n  </div>\r\n\r\n  <!-- Main content wrapper -->\r\n  <div class=\"content-wrapper\" id=\"contentWrapper\">\r\n    <!-- Your main content goes here -->\r\n  </div>\r\n\r\n  <script src=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"></script>\r\n  \r\n  <script>\r\n\r\n    //toggle button for sidebar when screen size is reduced\r\n    document.addEventListener('DOMContentLoaded', function() {\r\n      const sidebarToggle = document.getElementById('sidebarToggle');\r\n      const sidebar = document.getElementById('bedSidebar');\r\n      const contentWrapper = document.getElementById('contentWrapper');\r\n\r\n      sidebarToggle.addEventListener('click', function() {\r\n        sidebar.classList.toggle('active');\r\n        contentWrapper.classList.toggle('active');\r\n      });\r\n\r\n      // Close sidebar when clicking outside on mobile\r\n      document.addEventListener('click', function(event) {\r\n        const isClickInside = sidebar.contains(event.target) || \r\n                            sidebarToggle.contains(event.target);\r\n\r\n        if (!isClickInside && window.innerWidth <= 992 && \r\n            sidebar.classList.contains('active')) {\r\n          sidebar.classList.remove('active');\r\n          contentWrapper.classList.remove('active');\r\n        }\r\n      });\r\n\r\n      // Handle window resize\r\n      window.addEventListener('resize', function() {\r\n        if (window.innerWidth > 992) {\r\n          sidebar.classList.remove('active');\r\n          contentWrapper.classList.remove('active');\r\n        }\r\n      });\r\n    });\r\n  </script>\r\n</body>\r\n</html>\r\n\r\n\r\n  <script src=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"></script>"
        }
    ]
}